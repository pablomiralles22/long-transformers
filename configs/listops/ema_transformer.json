{
  "model_params": {
    "name": "layered",
    "params": {
      "input_embedding_dim": 16,
      "output_embedding_dim": 80,
      "layers": [
        [ "ema", { "dim": 16, "edim": 1, "out_dim": 160, "kernel_size": 2001, "direction": "forward" } ], [ "relu", {} ],
        [ "linear", { "in_features": 160, "out_features": 80 } ], [ "layer_norm", { "normalized_shape": 80 } ], [ "dropout", { "p": 0.1 } ],

        [ "rotary_transformer_encoder_layer", { "d_model": 80, "nhead": 8, "dim_feedforward": 160, "dropout": 0.2 } ],
        [ "rotary_transformer_encoder_layer", { "d_model": 80, "nhead": 8, "dim_feedforward": 160, "dropout": 0.2 } ],
        [ "rotary_transformer_encoder_layer", { "d_model": 80, "nhead": 8, "dim_feedforward": 160, "dropout": 0.2 } ],
        [ "rotary_transformer_encoder_layer", { "d_model": 80, "nhead": 8, "dim_feedforward": 160, "dropout": 0.2 } ]

      ]
    }
  },
  "head_params": {
    "ff_dim": 512,
    "num_hidden_layers": 1,
    "dropout_p": 0.1,
    "reduction_method": "cls"
  },
  "data_module_params": {
    "name": "listops",
    "params": {
      "data_path": "datasets/lra_release/listops-1000",
      "max_len": 2001,
      "batch_size": 32,
      "pin_memory": true,
      "num_workers": 4
    }
  },
  "optimizer_params": {
    "lr": 1e-3,
    "betas": [ 0.9, 0.98 ],
    "weight_decay": 0.01
  },
  "trainer_params": {
    "max_epochs": 60,
    "val_check_interval": 0.25,
    "precision": "16-mixed",
    "gradient_clip_val": 1.0,
    "enable_checkpointing": true,
    "default_root_dir": "out/listops/ema_transformer",
    "accelerator": "gpu",
    "devices": [ 1 ]
  },
  "fit_params": {}
}