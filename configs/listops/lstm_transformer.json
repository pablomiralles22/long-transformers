{
    "model_params": {
      "name": "layered",
      "params": {
          "input_embedding_dim": 128,
          "output_embedding_dim": 512,
          "layers": [
            [ "rnn", { "input_size": 128, "hidden_size": 512, "num_layers": 4, "nonlinearity": "relu", "dropout": 0.1, "batch_first": true } ]
          ]
      }
  },
    "head_params": {
      "ff_dim": 512,
      "num_hidden_layers": 2,
      "dropout_p": 0.2,
      "reduction_method": "mean"
    },
    "data_module_params": {
      "name": "listops",
      "params": {
        "data_path": "datasets/lra_release/listops-1000",
        "max_len": 2000,
        "batch_size": 512,
        "pin_memory": true,
        "num_workers": 4
      }
    },
    "optimizer_params": {
      "lr": 0.0001,
      "weight_decay": 0.01,
      "betas": [ 0.9, 0.999 ]
    },
    "trainer_params": {
      "max_epochs": 40,
      "precision": "bf16-mixed",
      "val_check_interval": 0.1,
      "gradient_clip_val": 1.0,
      "enable_checkpointing": true,
      "default_root_dir": "out/listops/lstm_transformer",
      "accelerator": "gpu",
      "devices": [ 2 ]
    },
    "fit_params": {}
  }
  