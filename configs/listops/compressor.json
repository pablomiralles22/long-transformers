{
  "model_params": {
    "name": "layered",
    "params": {
      "input_embedding_dim": 512,
      "output_embedding_dim": 512,
      "layers": [
        [ "positional_embedding", { "mode": "sinusoidal" } ],
        [ 
          "compressor",
          { "d_model": 512, "nhead": 8, "dim_feedforward": 2048, "mem_length": 512, "dropout": 0.2, "num_layers": 6 } 
        ]
      ]
    }
  },
  "head_params": {
    "ff_dim": 1024,
    "num_hidden_layers": 1,
    "dropout_p": 0.1,
    "reduction_method": "cls"
  },
  "data_module_params": {
    "name": "listops",
    "params": {
      "data_path": "datasets/lra_release/listops-1000",
      "max_len": 2000,
      "batch_size": 32,
      "pin_memory": true,
      "num_workers": 4
    }
  },
  "optimizer_params": {
    "lr": 5e-5,
    "betas": [ 0.9, 0.99 ],
    "weight_decay": 0.01
  },
  "trainer_params": {
    "max_epochs": 40,
    "precision": "16-mixed",
    "val_check_interval": 0.1,
    "gradient_clip_val": 1.0,
    "enable_checkpointing": true,
    "default_root_dir": "out/listops/compressor",
    "accelerator": "gpu",
    "devices": [ 2 ]
  },
  "fit_params": {}
}
