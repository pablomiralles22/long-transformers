{
  "model_params": {
    "name": "layered",
    "params": {
        "input_embedding_dim": 16,
        "output_embedding_dim": 128,
        "layers": [
          [ "conv", { "in_channels": 16, "out_channels": 64, "kernel_size": 7, "padding": "same" } ],
          [ "layer_norm", { "normalized_shape": 64 } ],
          [ "dropout", { "p": 0.2 } ],
          [ "relu", {} ],
  
          [ "conv", { "in_channels": 64, "out_channels": 128, "kernel_size": 5, "padding": "same" } ],
          [ "layer_norm", { "normalized_shape": 128 } ],
          [ "dropout", { "p": 0.2 } ],
          [ "relu", {} ],

          [ "conv", { "in_channels": 128, "out_channels": 256, "kernel_size": 5, "padding": "same" } ],
          [ "layer_norm", { "normalized_shape": 256 } ],
          [ "dropout", { "p": 0.2 } ],
          [ "relu", {} ],

          [ "linear", { "in_features": 256, "out_features": 128 } ],

          [ "rotary_transformer_encoder_layer", { "d_model": 128, "nhead": 8, "dim_feedforward": 256, "dropout": 0.2 } ],
          [ "rotary_transformer_encoder_layer", { "d_model": 128, "nhead": 8, "dim_feedforward": 256, "dropout": 0.2 } ],
          [ "rotary_transformer_encoder_layer", { "d_model": 128, "nhead": 8, "dim_feedforward": 256, "dropout": 0.2 } ],
          [ "rotary_transformer_encoder_layer", { "d_model": 128, "nhead": 8, "dim_feedforward": 256, "dropout": 0.2 } ],
          [ "rotary_transformer_encoder_layer", { "d_model": 128, "nhead": 8, "dim_feedforward": 256, "dropout": 0.2 } ],
          [ "rotary_transformer_encoder_layer", { "d_model": 128, "nhead": 8, "dim_feedforward": 256, "dropout": 0.2 } ]
        ]
    }
},
  "head_params": {
    "ff_dim": 512,
    "num_hidden_layers": 1,
    "dropout_p": 0.2,
    "reduction_method": "cls"
  },
  "data_module_params": {
    "name": "listops",
    "params": {
      "data_path": "datasets/lra_release/listops-1000",
      "max_len": 2000,
      "batch_size": 128,
      "pin_memory": true,
      "num_workers": 4
    }
  },
  "optimizer_params": {
    "lr": 5e-5,
    "weight_decay": 0.01,
    "betas": [ 0.9, 0.999 ]
  },
  "trainer_params": {
    "max_epochs": 40,
    "precision": "bf16-mixed",
    "val_check_interval": 0.25,
    "gradient_clip_val": 1.0,
    "enable_checkpointing": true,
    "default_root_dir": "out/listops/conv_transformer",
    "accelerator": "gpu",
    "devices": [ 1 ]
  },
  "fit_params": {}
}
