{
  "model_params": {
    "name": "layered",
    "params": {
      "input_embedding_dim": 16,
      "output_embedding_dim": 256,
      "layers": [
        [ "conv", { "in_channels": 16, "out_channels": 32, "kernel_size": 55, "padding": "same" } ],
        [ "layer_norm", { "normalized_shape": 32 } ],
        [ "dropout", { "p": 0.1 } ],
        [ "activation", { "fn": "gelu" } ],

        [ "conv", { "in_channels": 32, "out_channels": 64, "kernel_size": 55, "padding": "same" } ],
        [ "layer_norm", { "normalized_shape": 64 } ],
        [ "dropout", { "p": 0.1 } ],
        [ "activation", { "fn": "gelu" } ],

        [ "conv", { "in_channels": 64, "out_channels": 256, "kernel_size": 55, "padding": "same" } ],
        [ "layer_norm", { "normalized_shape": 256 } ],
        [ "dropout", { "p": 0.1 } ],
        [ "activation", { "fn": "gelu" } ]
      ]
    }
  },
  "head_params": {
    "ff_dim": 128,
    "num_hidden_layers": 0,
    "dropout_p": 0.0,
    "reduction_method": "mean"
  },
  "data_module_params": {
    "name": "listops",
    "params": {
      "data_path": "datasets/lra_release/listops-1000",
      "max_len": 2001,
      "augment": 1.0,
      "limit_tree_height": 5,
      "batch_size": 64,
      "pin_memory": true,
      "num_workers": 4
    }
  },
  "optimizer_params": {
    "lr": 1e-3,
    "betas": [ 0.9, 0.98 ],
    "weight_decay": 0.01
  },
  "trainer_params": {
    "max_epochs": 20,
    "val_check_interval": 0.25,
    "precision": "bf16-mixed",
    "enable_checkpointing": true,
    "default_root_dir": "out/listops/conv_augment_pretrain_1",
    "accelerator": "gpu",
    "devices": [ 2 ]
  },
  "fit_params": { }
}
