{
  "model_params": {
    "name": "layered",
    "params": {
      "input_embedding_dim": 32,
      "output_embedding_dim": 128,
      "layers": [
        [ "conv", { "in_channels": 32, "out_channels": 64, "kernel_size": 5, "padding": "same" } ],
        [ "layer_norm", { "normalized_shape": 64 } ],
        [ "dropout", { "p": 0.2 } ],
        [ "relu", {} ],

        [ "conv", { "in_channels": 64, "out_channels": 256, "kernel_size": 3, "padding": "same" } ],
        [ "layer_norm", { "normalized_shape": 256 } ],
        [ "dropout", { "p": 0.2 } ],
        [ "relu", {} ],

        [ "linear", { "in_features": 256, "out_features": 128 } ],

        [
          "residual_block",
          {
            "layer_params": [
              [ "linear", { "in_features": 128, "out_features": 128 } ],
              [ "layer_norm", { "normalized_shape": 128 } ],
              [ "dropout", { "p": 0.3 } ],
              [ "relu", {} ],
              [ "linear", { "in_features": 128, "out_features": 128 } ],
              [ "layer_norm", { "normalized_shape": 128 } ]
            ]
          }
        ],

        [
          "residual_block",
          {
            "layer_params": [
              [ "linear", { "in_features": 128, "out_features": 128 } ],
              [ "layer_norm", { "normalized_shape": 128 } ],
              [ "dropout", { "p": 0.3 } ],
              [ "relu", {} ],
              [ "linear", { "in_features": 128, "out_features": 128 } ],
              [ "layer_norm", { "normalized_shape": 128 } ]
            ]
          }
        ],

        [ "positional_encoding", {} ],
        [ 
          "compressor",
          { "d_model": 128, "nhead": 16, "dim_feedforward": 256, "vdim": 256, "dropout": 0.5, "mem_length": 128, "num_layers": 2 } 
        ]
      ]
    }
  },
  "head_params": {
    "ff_dim": 512,
    "num_hidden_layers": 1,
    "dropout_p": 0.2,
    "reduction_method": "cls"
  },
  "data_module_params": {
    "name": "text-classification",
    "params": {
      "data_path": "datasets/aclImdb",
      "max_len": 4096,
      "batch_size": 8,
      "pin_memory": true,
      "num_workers": 4
    }
  },
  "optimizer_params": {
    "lr": 5e-5,
    "betas": [ 0.9, 0.99 ],
    "weight_decay": 0.01
  },
  "trainer_params": {
    "max_epochs": 30,
    "precision": "16-mixed",
    "enable_checkpointing": true,
    "default_root_dir": "out/text_classification/compressor",
    "accelerator": "gpu",
    "devices": [ 2 ]
  }
}
